#
# ~/.bash_profile
#

[[ -f ~/.bashrc ]] && . ~/.bashrc

export GPG_TTY=$(tty)
export EDITOR="nvim"
export READER="zathura"
export TERMINAL="st"
export BROWSER="firefox"


